#: ../src/tags/gnome-cmd-tags.cc:525
msgid "Document Assembly"
msgstr "Sastavljanje dokumenta"

#: ../src/tags/gnome-cmd-tags.cc:525
msgid ""
"Set to \"1\" if inserting, rotating, or deleting pages and creating "
