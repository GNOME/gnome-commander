msgstr "Ogniskowa dla filmu 35 mm."

#: src/tags/gnome-cmd-tags.cc:430
msgid "Height"
msgstr "Wysokość"

#: src/tags/gnome-cmd-tags.cc:430
msgid "Height in pixels."
msgstr "Wysokość w pikselach."

#: src/tags/gnome-cmd-tags.cc:431
msgid "ISO Speed"
msgstr "Czułość ISO"

