msgstr "Organizație"

#: src/tags/gnome-cmd-tags.cc:541
msgid "Organization producing the track."
msgstr "Organizația producătoare a piesei."

#: src/tags/gnome-cmd-tags.cc:542
msgid "Vendor"
msgstr "Vânzător"

#: src/tags/gnome-cmd-tags.cc:542
