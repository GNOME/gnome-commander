msgid "InvolvedPeople2"
msgstr "InvolvedPeople2"

#: ../src/tags/gnome-cmd-tags.cc:358
msgid "Language."
msgstr "Språk."

#: ../src/tags/gnome-cmd-tags.cc:359
msgid "Linked Info"
